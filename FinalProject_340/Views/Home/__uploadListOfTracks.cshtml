@model new_list_tracks;
@{
}
@using (Html.BeginForm("uploadMultiples", "api", FormMethod.Post, new
{
    enctype = "multipart/form-data"
}))
{
        <td colspan="1">

        </td>
        <td class="text-end"><partial name="__toggleUploadAsList" view-data="ViewData"></partial></td>
        <td colspan="10"><input type="text" placeholder="Title" disabled/></td>
        <td colspan="1"><input type="text" placeholder="Artist" disabled /></td>
        <td colspan="2"><input type="text" placeholder="Album" disabled /></td>
        <td colspan="2" style="cursor:pointer">
            <img src="~/images/new-document.png" height="20" width="20" id="_input" />
        <input type="file" asp-for="songFiles" accept=".mp3,.m4a,.wav,.ogg" id="input" style="display: none" multiple />
            <img src="~/images/picture.png" height="20" width="20" id="_inputImg" />
        <input type="file" asp-for="albumArt" accept=".jpg,.png" id="inputImg" style="display: none" single />
        </td>
        <td colspan="1" style="cursor:pointer">
            <button type="submit" style="padding: 0; border: none; background-color: rgba(0,0,0,0)"><img src="~/images/upload.png" height="20" width="20" id="_uploadbtn" /></button>
        </td>
}